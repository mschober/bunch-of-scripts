#http://nathanhoad.net/how-to-colours-in-git

[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
#  added = yellow
  added = green
#  changed = green
  changed = red
  untracked = cyan
